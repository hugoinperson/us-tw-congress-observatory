    "build:prod": "NODE_ENV=production npm-run-all clean lint build",
    "build": "webpack --progress --colors --config ./webpack/webpack.config.js",
    "test": "mocha --compilers js:babel-register --require ./src/utils/testSetup.js --reporter spec --full-trace ./src/**/*.test.js",
    "test:watch": "npm run test -- --watch"
